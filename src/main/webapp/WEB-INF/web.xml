<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddNotesServlet</servlet-name>
  	<display-name>AddNotesServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.AddNotesServlet</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>ToDeleteServlet</servlet-name>
  	<display-name>ToDeleteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.ToDeleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet</servlet-name>
  	<display-name>UpdateServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddNotesServlet</servlet-name>
  	<url-pattern>/AddNotesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ToDeleteServlet</servlet-name>
  	<url-pattern>/ToDeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet</servlet-name>
  	<url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>
</web-app>
